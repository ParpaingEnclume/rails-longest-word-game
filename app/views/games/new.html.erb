<h1>Games#new</h1>
<p>Find me in app/views/games/new.html.erb</p>
<%= @letters %>
<form action="/score" method="post">
<label for="guess">What is the longest word you can find?</label>
  <%= hidden_field_tag :authenticity_token, form_authenticity_token %>
  <%= hidden_field_tag :hidden_array, @letters %>
  <br>
  <input type="text" name="guess">
  <input type="submit" value="score">
</form>
